{
  "name": "boorsound",
  "productName": "Boorsound",
  "version": "1.0.0",
  "description": "An audio player for desktop",
  "main": "main.js",
  "scripts": {
    "start": "electron .",
    "package-win": "electron-packager . Boorsound --overwrite --asar=true --platform=win32 --arch=x64 --icon=./assets/icons/icon.ico --prune=true --out=build --version-string.CompanyName=Boorsoft --version-string.FileDescription=An audio player --version-string.ProductName=\"Boorsound\"",
    "package-linux": "electron-packager . Boorsound --overwrite --asar=true --platform=linux --arch=x64 --icon=./assets/icons/icon.png --prune=true --out=build"
  },
  "author": "boorsoft",
  "license": "ISC",
  "devDependencies": {
    "electron": "^10.1.5",
    "electron-packager": "^15.1.0",
    "electron-winstaller": "^4.0.1"
  },
  "dependencies": {
    "electron-reload": "^1.5.0",
    "musicmetadata": "^2.0.5"
  }
}
